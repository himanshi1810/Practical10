
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        Employee List
        @foreach (var employee in Model)
        {
            <div>
                <span>@employee.Id</span>
                <span>@employee.Name</span>
            </div>
        }
    </div>
</body>
</html>
